%%%% Log Api -----------------------------------------------------------------------------

%% log:debug(Msg)
-define(LOG_DEBUG(Msg),log:debug(Msg,?Sender)).

-define(LOG_NOTICE(Msg),log:notice(Msg,?Sender)).

-define(LOG_WARNING(Msg),log:warning(Msg,?Sender)).

-define(LOG_ALERT(Msg),log:alert(Msg,?Sender)).
